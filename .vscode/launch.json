{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug_nobuild",
      "type": "cppdbg",
      "request": "launch",
      "args": [
        "pull",
        "--format",
        "oci",
        "--os",
        "linux",
        "--arch",
        "amd64",
        "--all-tags",
        "10.68.1.145:5000/library/busybox-image:latest"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "linux": {
        "MIMode": "gdb",
        "miDebuggerPath": "gdb",
        "program": "${workspaceFolder}/output/main"
      },
      "osx": {
        "MIMode": "lldb",
        "miDebuggerPath": "lldb-mi",
        "program": "${workspaceFolder}/output/main"
      },
      "windows": {
        "MIMode": "gdb",
        "miDebuggerPath": "D:\\softwares\\mingw64\\bin\\gdb.exe",
        "program": "${workspaceFolder}/output/main.exe"
      },
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "logging": {
        "engineLogging": true,
        "trace": true,
        "exceptions": true
      },
      "preLaunchTask": ""
    },
    {
      "name": "Debugandbuild",
      "type": "cppdbg",
